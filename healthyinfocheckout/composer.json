{
  "name": "prestashop/healthyinfocheckout",
  "description": "PrestaShop - healthyinfocheckout",
    "homepage": "https://github.com/tesshsu/healthyinfocheckout",
    "license": "AFL-3.0",
    "type": "prestashop-module",
    "authors": [
      {
        "name": "Tess",
        "email": "tesshsu@gmail.com"
      }
    ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\HealthyInfoCheckout\\": "src/"
    },
    "classmap": [
      "healthyinfocheckout.php",
      "controllers",
      "classes"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "prepend-autoloader": false,
    "platform": {
      "php": "7.2.0"
    }
  }
}

